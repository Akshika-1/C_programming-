#include<stdio.h>
int main()
{
    int score;
    printf("Enter your score ");
    scanf("%d",&score);
  
    if(score<40)
    {
        printf("Grade F");
    }
    else if(score>=40 && score<54)
    {
        printf("Grade D");
    }
    else if(score>=55 && score<69)
    {
        printf("Grade C");
    }
    else if(score>=70 && score<84)
    {
        printf("Grade B");
    }
    else if(score>=85 && score<100)
    {
        printf("Grade A");
    }
    else
    {
        printf("Invalid Score");
    }
    return 0;
}
